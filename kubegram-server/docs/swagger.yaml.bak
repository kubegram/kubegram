  /auth/{provider}/callback:
    get:
      description: Handles the callback from the OAuth provider, exchanges code for
        token, and redirects to frontend
      parameters:
      - description: OAuth provider (github, gitlab, google)
        in: path
        name: provider
        required: true
        type: string
      - description: Authorization code
        in: query
        name: code
        required: true
        type: string
      - description: State parameter
        in: query
        name: state
        required: true
        type: string
      responses:
        "302":
          description: Redirect to frontend
          schema:
            type: string
        "400":
          description: Bad Request
          schema:
            additionalProperties:
              type: string
            type: object
        "401":
          description: Unauthorized
          schema:
            additionalProperties:
              type: string
            type: object
        "500":
          description: Internal Server Error
          schema:
            additionalProperties:
              type: string
            type: object
      summary: OAuth callback
      tags:
      - auth
  /auth/{provider}/login:
    get:
      description: Redirects to the OAuth provider's login page
      parameters:
      - description: OAuth provider (github, gitlab, google)
        in: path
        name: provider
        required: true
        type: string
      responses:
        "302":
          description: Redirect to provider
          schema:
            type: string
        "404":
          description: Not Found
          schema:
            additionalProperties:
              type: string
            type: object
      summary: Start OAuth flow
      tags:
      - auth
  /auth/{provider}/refresh:
    post:
      description: Refreshes the access token using the stored refresh token
      parameters:
      - description: OAuth provider (github, gitlab, google)
        in: path
        name: provider
        required: true
        type: string
      - description: User ID
        in: body
        name: request
        required: true
        schema:
          additionalProperties:
            type: integer
          type: object
      responses:
        "200":
          description: OK
          schema:
            additionalProperties: true
            type: object
        "400":
          description: Bad Request
          schema:
            additionalProperties:
              type: string
            type: object
        "401":
          description: Unauthorized
          schema:
            additionalProperties:
              type: string
            type: object
        "404":
          description: Not Found
          schema:
            additionalProperties:
              type: string
            type: object
      summary: Refresh OAuth token
      tags:
      - auth
