# Environment Variables

# GraphQL Endpoints
# HTTP endpoint for GraphQL queries and mutations
VITE_GRAPHQL_HTTP_URL=http://localhost:4000/graphql

# WebSocket endpoint for GraphQL subscriptions (real-time updates)
VITE_GRAPHQL_WS_URL=ws://localhost:4000/graphql

# OAuth Backend API
# Base URL for the OAuth backend API
VITE_API_URL=http://localhost:8090

# OIDC Configuration
# Client ID for OpenID Connect provider
VITE_OIDC_CLIENT_ID=your_oidc_client_id

# OIDC Issuer URL
VITE_OIDC_ISSUER=https://your-oidc-provider.com
