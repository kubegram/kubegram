{
  "name": "@kubegram/kubegram-auth",
  "version": "0.1.0",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "description": "OAuth authentication library built on OpenAuth.js with multiple provider support",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "lint": "eslint src/**/*.ts --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts' --no-warn-ignored",
    "lint:fix": "eslint src/**/*.ts --ignore-pattern '**/__tests__/**' --ignore-pattern '**/*.test.ts' --no-warn-ignored --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint && npm run format:check"
  },
  "keywords": [
    "oauth",
    "authentication",
    "auth",
    "openauth",
    "oauth2",
    "typescript"
  ],
  "author": "Kubegram",
  "license": "BUSL-1.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/kubegram/kubegram.git",
    "directory": "kubegram-auth"
  },
  "bugs": {
    "url": "https://github.com/kubegram/kubegram/issues"
  },
  "homepage": "https://github.com/kubegram/kubegram/tree/main/kubegram-auth#readme",
  "devDependencies": {
    "@types/node": "^24.10.13",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@kubegram/common-events": "file:../common-events",
    "@openauthjs/openauth": "^0.4.3",
    "valibot": "^0.42.0"
  },
  "peerDependencies": {
    "ioredis": "^5.4.0",
    "lru-cache": "^11.0.0",
    "redis": "^4.7.1"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  }
}
